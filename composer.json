{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": ">=2.0.5",
    "yiisoft/yii2-bootstrap": "*",
    "rmrevin/yii2-minify-view": "^1.15",
    "kartik-v/yii2-widget-select2": "@dev",
    "kartik-v/yii2-widget-datetimepicker": "*",
    "kartik-v/yii2-widget-datepicker": "*",
    "kartik-v/yii2-widget-colorinput": "*",
    "kartik-v/yii2-widget-fileinput": "*",
    "kartik-v/yii2-datecontrol": "^1.9",
    "voskobovich/yii2-linker-behavior": "^4.0",
    "bower-asset/handlebars": "^4.0",
    "bower-asset/spectrum": "^1.8",
    "bower-asset/snackbarjs": "dev-fixes",
    "bower-asset/js-cookie": "^2.1",
    "bower-asset/chart.js": ">=2.5",
    "bower-asset/fontawesome-iconpicker": "^1.3",
    "bower-asset/masonry": "^4.2",
    "cboden/ratchet": "^0.4.0",
    "ratchet/pawl": "^0.3.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      },
      "repositories": [
        {
          "type": "bower-github",
          "url": "https://github.com/CyanoFresh/snackbarjs",
          "name": "bower-asset/snackbarjs"
        }
      ]
    }
  },
  "scripts": {
    "post-install-cmd": "php init --env=Development --overwrite=n"
  }
}
